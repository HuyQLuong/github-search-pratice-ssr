    "react-router-dom": "^6.2.2",
    "styled-components": "^5.3.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "build:server": "webpack --env=server",
    "build:client": "webpack --env=web",
    "start:ssr": "webpack && node ./build/server.js",
